{
  "name": "abnf-ts",
  "version": "0.1.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "npm run node",
    "prepublishOnly": "npm test && npm run tsc",
    "tsc": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "test": "npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pascal-brauns/abnf-ts.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "pascal-brauns"
  },
  "bugs": "https://github.com/pascal-brauns/abnf-ts/issues",
  "keywords": [
    "abnf"
  ],
  "license": "MIT",
  "description": "A type-safe, performant and easy to use ABNF (Augmented Backus-Naur Form) engine.",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
